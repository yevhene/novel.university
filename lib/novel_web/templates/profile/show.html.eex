<h2>Profile</h2>

<ul>
  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>
  <li>
    <strong>Nickname:</strong>
    <%= @user.nickname %>
  </li>
</ul>

<ul>
  <li>
    <strong>First Name:</strong>
    <%= @user.first_name %>
  </li>
  <li>
    <strong>Last Name:</strong>
    <%= @user.last_name %>
  </li>
</ul>

<span><%= link "Edit", to: profile_path(@conn, :edit) %></span>
